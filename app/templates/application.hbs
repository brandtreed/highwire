<div class="wrapper">
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">NowWhat</a>
        </div>
        <div class="collapse navbar-collapse navbar-ex1-collapse">
        </div>
    </nav>
    <div class="container" id="main">
        <div class="row">
            <div class="sub-row">
                <div class="col-md-1 left-side-nav">
                    <div class="sidebar-nav">
                        <div class="icon-list">
                            <ul class="nav nav-tabs" role="tablist" id="myTab">
                                <li class="icon icon-index active">
                                    <a href="#index" role="tab" data-toggle="tab">
                                        <span class="tab-text">index</span>
                                    </a>
                                </li>
                                <li class="icon icon-new">
                                    <a href="#new" role="tab" data-toggle="tab">
                                        <span class="tab-text">new</span>
                                    </a>
                                </li>
                                <li class="icon icon-edit">
                                    <a href="#edit" role="tab" data-toggle="tab">
                                        <span class="tab-text">edit</span>
                                    </a>
                                </li>
                                <li class="icon icon-sync">
                                    <a href="#sync" role="tab" data-toggle="tab">
                                        <span class="tab-text">sync</span>
                                    </a>
                                </li>
                                <li class="icon icon-clone">
                                    <a href="#clone" role="tab" data-toggle="tab">
                                        <span class="tab-text">clone</span>
                                    </a>
                                </li>
                                <li class="icon icon-delete">
                                    <a href="#delete" role="tab" data-toggle="tab">
                                        <span class="tab-text">delete</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 well side-forms">
                    <div class="tab-content">
                        <div class="tab-pane fade in active" id="index">
                            <div class="search-form">
                                <div class="select-search">
                                    {{view Ember.Select content=objectNames}}
                                    {{input type="text" value="search" placeholder="Search" size="25"}}
                                </div>
                                {{input type="submit" value="search" class="button full-input"}}
                            </div>
                            <p>To add components, drag to canvas on the right.</p>
                            <div class="search-results">
                            </div>
                        </div>
                        <div class="tab-pane fade" id="new">
                            <label for="new-domain">Domain Name</label>
                            {{input type="text" class="full-input" id="new-domain" value="domain" placeholder="Domain"}}
                            <label for="new-jcode">jCode</label>
                            {{input type="text" class="full-input" id="new-jcode" value="jcode" placeholder="jCode"}}
                        </div>
                        <div class="tab-pane fade" id="edit">
                            <label for="edit-domain">Domain Name</label>
                            {{input type="text" class="full-input" id="edit-domain" value="domain" placeholder="Domain"}}
                            <label for="edit-jcode">jCode</label>
                            {{input type="text" class="full-input" id="edit-jcode" value="jcode" placeholder="jCode"}}
                        </div>
                        <div class="tab-pane fade" id="sync">
                            {{input type="button" class="button full-input" value="Dev"}}
                            {{input type="button" class="button full-input" value="Staging"}}
                            {{input type="button" class="button full-input" value="QA"}}
                            {{input type="button" class="button full-input" value="Prod"}}
                        </div>
                        <div class="tab-pane fade" id="clone">
                            {{input type="button" class="button full-input" value="clone"}}
                        </div>
                        <div class="tab-pane fade" id="delete">
                            {{input type="button" class="button full-input" value="delete"}}
                        </div>
                    </div>
                </div>
                <div class="col-md-8 main-content-outlet">
                    {{outlet}}
                </div>
            </div>
        </div>
    </div>
</div>
